{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "strict": true,
      "jsx": "react-native",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "allowJs": true,
      "noEmit": true,
      "outDir": "./dist"
    },
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "node_modules",
      "babel.config.js",
      "metro.config.js",
      "jest.config.js"
    ]
}
  